{
  "name": "rntsmobx",
  "version": "1.6.0",
  "private": true,
  "scripts": {
    "start": "react-native start",
    "test": "jest",
    "fix": "tslint --fix -c ./tslint.json 'src/**/*{.ts,.tsx}'",
    "android": "react-native run-android",
    "ios": "react-native run-ios",
    "release": "cd android && ./gradlew bundleRelease",
    "check": "prettier --check 'src/**/*.tsx' && prettier --check 'src/**/*.ts'",
    "npm:update": "npm upgrade --global yarn && yarn global remove npm-check-updates && yarn global add npm-check-updates && ncu -a -u && rm -rf node_modules yarn.lock && yarn",
    "prettier": "prettier --write \"{,!(node_modules|dist|build|coverage)/**/}*.{js,jsx,ts,tsx,json}\"",
    "pre-commit": "yarn tslint:fix && yarn prettier && yarn build && yarn test",
    "tslint": "tslint --project tsconfig.json",
    "tslint:fix": "tslint --project tsconfig.json --fix",
    "run-android:debug": "react-native run-android",
    "run-android:debugRelease": "react-native run-android --variant=release",
    "run-ios:debug": "react-native run-ios --scheme rntsmobx",
    "run-ios:debugRelease": "react-native run-ios --scheme rntsmobx --configuration Release",
    "fastlane": "bundle exec fastlane",
    "uninstall-android": "adb uninstall com.rntsmobx",
    "clear-android": "adb shell pm clear com.rntsmobx",
    "uninstall-iOS": "xcrun simctl uninstall booted com.rntsmobx",
    "pixel3a": "emulator @Pixel_3a_XL_API_29",
    "run-ios:debug:iPhone8": "react-native run-ios --simulator=\"iPhone 8\" --scheme rntsmobx",
    "adb-metro": "adb reverse tcp:8081 tcp:8081",
    "adb-reactotron": "adb reverse tcp:9090 tcp:9090",
    "git:add": "git add android/app/build.gradle ios/rntsmobx.xcodeproj/project.pbxproj ios/rntsmobx/Info.plist package.json",
    "git:commit": "git commit -m 'bump version app'",
    "git:push": "git push && git push --tags",
    "bump-patch": "npm version patch --no-git-tag-version && bundle exec fastlane bump_badge && yarn git:add && yarn git:commit && yarn git:push",
    "bump-minor": "npm version minor --no-git-tag-version && bundle exec fastlane bump_badge && yarn git:add && yarn git:commit && yarn git:push",
    "bump-major": "npm version major --no-git-tag-version && bundle exec fastlane bump_badge && yarn git:add && yarn git:commit && yarn git:push"
  },
  "dependencies": {
    "@react-native-community/async-storage": "^1.11.0",
    "@react-native-community/datetimepicker": "^2.5.0",
    "@react-native-community/google-signin": "^4.0.3",
    "@react-native-community/masked-view": "^0.1.0",
    "@react-native-community/netinfo": "5.0.0",
    "@react-native-community/push-notification-ios": "^1.3.0",
    "@react-native-community/viewpager": "^3.3.0",
    "@react-native-firebase/analytics": "^6.7.2",
    "@react-native-firebase/app": "^6.7.1",
    "@react-native-firebase/crashlytics": "^6.7.1",
    "@react-native-firebase/messaging": "^6.7.1",
    "@react-native-firebase/perf": "^6.7.3",
    "@react-navigation/bottom-tabs": "^5.6.1",
    "@react-navigation/compat": "^5.1.28",
    "@react-navigation/material-bottom-tabs": "^5.2.12",
    "@react-navigation/native": "^5.6.1",
    "@react-navigation/stack": "^5.6.2",
    "autobind-decorator": "^2.4.0",
    "axios": "^0.19.2",
    "buffer": "^5.6.0",
    "google-libphonenumber": "^3.2.10",
    "i18n-js": "^3.7.1",
    "lodash": "^4.17.15",
    "mobx": "^5.15.4",
    "mobx-logger": "^0.7.1",
    "mobx-react": "^6.2.2",
    "moment": "2.26.0",
    "moment-timezone": "^0.5.31",
    "pluralize": "^8.0.0",
    "react": "16.13.1",
    "react-devtools": "^4.7.0",
    "react-native": "0.62.2",
    "react-native-appearance": "^0.3.4",
    "react-native-autoheight-webview": "^1.5.1",
    "react-native-calendars": "^1.300.0",
    "react-native-device-info": "^5.6.1",
    "react-native-document-picker": "^3.5.3",
    "react-native-emoji-selector": "^0.1.9",
    "react-native-event-listeners": "^1.0.7",
    "react-native-exit-app": "^1.1.0",
    "react-native-fs": "^2.16.6",
    "react-native-gesture-handler": "^1.6.1",
    "react-native-htmlview": "^0.15.0",
    "react-native-image-picker": "^2.3.2",
    "react-native-keyboard-tracking-view": "^5.7.0",
    "react-native-linear-gradient": "^2.5.6",
    "react-native-localize": "^1.4.0",
    "react-native-mentions-editor": "^1.0.11",
    "react-native-mixpanel": "^1.2.0",
    "react-native-modal": "^11.5.6",
    "react-native-modal-datetime-picker": "^8.7.1",
    "react-native-pages": "^0.9.0",
    "react-native-permissions": "^2.1.5",
    "react-native-push-notification": "^3.5.2",
    "react-native-reanimated": "^1.9.0",
    "react-native-safe-area-context": "^0.7.3",
    "react-native-screens": "^2.9.0",
    "react-native-snap-carousel": "^3.9.1",
    "react-native-spinkit": "^1.5.0",
    "react-native-splash-screen": "^3.2.0",
    "react-native-svg": "^12.1.0",
    "react-native-swiper": "1.6.0",
    "react-native-tab-view": "2.14.2",
    "react-native-user-agent": "^2.1.0",
    "react-native-vector-icons": "^6.7.0",
    "react-native-video": "^4.4.5",
    "react-native-video-player": "^0.10.1",
    "react-native-walkthrough-tooltip": "^1.1.9",
    "react-native-webview": "^9.4.0",
    "rn-fetch-blob": "^0.11.2",
    "rn-placeholder": "3.0.0"
  },
  "devDependencies": {
    "@babel/core": "^7.10.4",
    "@babel/plugin-proposal-decorators": "^7.10.4",
    "@babel/runtime": "^7.10.4",
    "@react-native-community/cli-debugger-ui": "^4.9.0",
    "@types/axios": "^0.14.0",
    "@types/i18n-js": "^3.0.3",
    "@types/jest": "^25.2.3",
    "@types/lodash": "^4.14.157",
    "@types/qs": "^6.9.3",
    "@types/react": "^16.9.41",
    "@types/react-native": "^0.61.5",
    "@types/react-native-background-timer": "^2.0.0",
    "@types/react-native-permissions": "^2.0.0",
    "@types/react-native-snap-carousel": "^3.8.1",
    "@types/react-native-vector-icons": "^6.4.5",
    "@types/react-test-renderer": "^16.9.2",
    "babel-jest": "^25.5.1",
    "babel-plugin-inline-import": "^3.0.0",
    "babel-plugin-module-resolver": "3.1.1",
    "babel-plugin-transform-remove-console": "6.9.4",
    "babel-preset-mobx": "^2.0.0",
    "enzyme": "^3.11.0",
    "jest": "^25.5.4",
    "metro-react-native-babel-preset": "^0.58.0",
    "metro-react-native-babel-transformer": "^0.59.0",
    "react-addons-test-utils": "^15.6.2",
    "react-native-svg-transformer": "^0.13.0",
    "react-native-typescript-transformer": "^1.2.11",
    "react-test-renderer": "16.13.1",
    "reactotron-react-native": "^3.6.4",
    "ts-jest": "^25.5.1",
    "tslint": "^6.1.2",
    "tslint-react-native": "^0.0.7",
    "typescript": "^3.9.6"
  },
  "jest": {
    "preset": "react-native",
    "moduleFileExtensions": [
      "ts",
      "tsx",
      "js",
      "ios.ts",
      "android.ts",
      "ios.tsx",
      "android.tsx"
    ],
    "transform": {
      "^.+\\.(js)$": "<rootDir>/node_modules/babel-jest",
      "\\.(ts|tsx)$": "<rootDir>/node_modules/ts-jest/preprocessor.js"
    },
    "testRegex": "(/__tests__/.*|\\.(test|spec))\\.(ts|tsx|js)$",
    "testPathIgnorePatterns": [
      "\\.snap$",
      "<rootDir>/node_modules/"
    ],
    "cacheDirectory": ".jest/cache"
  },
  "rnpm": {
    "assets": [
      "./src/assets/fonts/"
    ]
  },
  "husky": {
    "hooks": {
      "pre-commit": "yarn pre-commit"
    }
  }
}
